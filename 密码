#include<iostream>
#include<string>
using namespace std;
bool checkPassword(string password)
{
	if (password.length() < 8)
	{
		return false;
	}
	int a = 0, b = 0,c=0;
	for (int i = 0;i < password.length();i++)
	{
		if (password[i] >= 'a'&& password[i] <= 'z'|| password[i] >= 'A' && password[i] <= 'Z') {
			a ++;
		}
		else
		if (password[i] >= '0' && password[i] <= '9') {
			b++;
		}
		else
		{
			c++;
		}
	}
	if (b < 2) {
		return false;
	}
	if (a < 1) {
		return false;
	}
	if (c > 0) {
		return false;
	}
}
int main()
{
	std::cout << "Please put in your password:" << std::endl;
	string pd;
	while (cin >> pd) {
		if (checkPassword(pd)) {
			cout << "True password" << endl;
		}
		else
		{
			cout << "False password" << endl;
		}
	}
	return 0;
}
